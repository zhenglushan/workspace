<?xml version="1.0" encoding="utf-8"?>
<urlset content_method="full">
	<url><!-- 不超过 1000 条 -->
		<loc><![CDATA[内容 URL 地址]]></loc><!-- 修改 -->
		<lastmod><![CDATA[2017-01-01]]></lastmod><!-- 修改, 最后更新年月日 -->
		<changefreq><![CDATA[weekly]]></changefreq><!-- 固定 -->
		<priority><![CDATA[1.0]]></priority><!-- 固定 -->
		<data>
			<display>
				<pic_item>
					<pic_url><![CDATA[图片地址]]></pic_url><!-- 修改 -->
					<pic_from_url><![CDATA[图片所在网页地址]]></pic_from_url><!-- 修改 -->
					<pic_title><![CDATA[图片标题]]></pic_title><!-- 修改 -->
					<pic_type><![CDATA[设计]]></pic_type><!-- 固定, 稿定设计用设计, 稿定素材用模板 -->
					<pic_tag><![CDATA[美女@100|摄影@90]]></pic_tag><!-- 修改, 格式 tag@分值|tag@分值 分值为 1~100 整型 -->
					<pic_score><![CDATA[80 ~ 100 随机值]]></pic_score><!-- 修改 -->
					<pic_desc><![CDATA[简介或者描述]]></pic_desc><!-- 修改 -->
					<update_time><![CDATA[更新的时间戳]]></update_time><!-- 修改 -->
				</pic_item>
				<provider>
					<site_name><![CDATA[稿定设计]]></site_name><!-- 固定 -->
					<show_site><![CDATA[www.gaoding.com]]></show_site><!-- 固定, PC 端写 www, M 端写 m -->
				</provider>
			</display>
		</data>
	</url>
</urlset>